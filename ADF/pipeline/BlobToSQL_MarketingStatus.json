{
	"name": "BlobToSQL_MarketingStatus",
	"properties": {
		"activities": [
			{
				"name": "Copy Data File to SQL",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "BlobSource",
						"recursive": false
					},
					"sink": {
						"type": "SqlSink",
						"writeBatchSize": 10000,
						"preCopyScript": "TRUNCATE TABLE dbo.MarketingStatus"
					},
					"enableStaging": false,
					"dataIntegrationUnits": 0,
					"translator": {
						"type": "TabularTranslator",
						"columnMappings": {
							"MarketingStatusID": "MarketingStatusID",
							"ApplNo": "ApplNo",
							"ProductNo": "ProductNo"
						}
					}
				},
				"inputs": [
					{
						"referenceName": "AzureBlob_File_MarketingStatus",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureSQLDB_TableMarketingStatus",
						"type": "DatasetReference"
					}
				]
			}
		]
	}
}