{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "jejohnmylanadf"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/BlobToSQL_MarketingStatus')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy Data File to SQL",
						"type": "Copy",
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"typeProperties": {
							"source": {
								"type": "BlobSource",
								"recursive": false
							},
							"sink": {
								"type": "SqlSink",
								"writeBatchSize": 10000,
								"preCopyScript": "TRUNCATE TABLE dbo.MarketingStatus"
							},
							"enableStaging": false,
							"dataIntegrationUnits": 0,
							"translator": {
								"type": "TabularTranslator",
								"columnMappings": {
									"MarketingStatusID": "MarketingStatusID",
									"ApplNo": "ApplNo",
									"ProductNo": "ProductNo"
								}
							}
						},
						"inputs": [
							{
								"referenceName": "AzureBlob_File_MarketingStatus",
								"type": "DatasetReference"
							}
						],
						"outputs": [
							{
								"referenceName": "AzureSQLDB_TableMarketingStatus",
								"type": "DatasetReference"
							}
						]
					}
				],
				"folder": {
					"name": "DB Loads"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/AzureBlob_File_MarketingStatus')]",
				"[concat(variables('factoryId'), '/datasets/AzureSQLDB_TableMarketingStatus')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/BlobToSQL_Products')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy Data File to SQL",
						"type": "Copy",
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"typeProperties": {
							"source": {
								"type": "BlobSource",
								"recursive": false
							},
							"sink": {
								"type": "SqlSink",
								"writeBatchSize": 10000,
								"preCopyScript": "TRUNCATE TABLE dbo.Products"
							},
							"enableStaging": false,
							"dataIntegrationUnits": 0,
							"translator": {
								"type": "TabularTranslator",
								"columnMappings": {
									"ApplNo": "ApplNo",
									"ProductNo": "ProductNo",
									"Form": "Form",
									"Strength": "Strength",
									"ReferenceDrug": "ReferenceDrug",
									"DrugName": "DrugName",
									"ActiveIngredient": "ActiveIngredient",
									"ReferenceStandard": "ReferenceStandard"
								}
							}
						},
						"inputs": [
							{
								"referenceName": "AzureBlob_File_Products",
								"type": "DatasetReference"
							}
						],
						"outputs": [
							{
								"referenceName": "AzureSQLDB_Table_Products",
								"type": "DatasetReference"
							}
						]
					}
				],
				"folder": {
					"name": "DB Loads"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/AzureBlob_File_Products')]",
				"[concat(variables('factoryId'), '/datasets/AzureSQLDB_Table_Products')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureBlob_File_MarketingStatus')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "LinkedService_jejohnmylanstorage",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "Blob Datasets"
				},
				"type": "AzureBlob",
				"structure": [
					{
						"name": "MarketingStatusID",
						"type": "Int32"
					},
					{
						"name": "ApplNo",
						"type": "Int32"
					},
					{
						"name": "ProductNo",
						"type": "Int32"
					}
				],
				"typeProperties": {
					"format": {
						"type": "TextFormat",
						"columnDelimiter": "\t",
						"rowDelimiter": "",
						"nullValue": "\\N",
						"encodingName": null,
						"treatEmptyAsNull": true,
						"skipLineCount": 0,
						"firstRowAsHeader": true
					},
					"fileName": "MarketingStatus.txt",
					"folderPath": "extracts"
				}
			}
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureBlob_File_Products')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "LinkedService_jejohnmylanstorage",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "Blob Datasets"
				},
				"type": "AzureBlob",
				"structure": [
					{
						"name": "ApplNo",
						"type": "Int32"
					},
					{
						"name": "ProductNo",
						"type": "Int32"
					},
					{
						"name": "Form",
						"type": "String"
					},
					{
						"name": "Strength",
						"type": "String"
					},
					{
						"name": "ReferenceDrug",
						"type": "Int32"
					},
					{
						"name": "DrugName",
						"type": "String"
					},
					{
						"name": "ActiveIngredient",
						"type": "String"
					},
					{
						"name": "ReferenceStandard",
						"type": "Int32"
					}
				],
				"typeProperties": {
					"format": {
						"type": "TextFormat",
						"columnDelimiter": "\t",
						"rowDelimiter": "",
						"nullValue": "\\N",
						"encodingName": null,
						"treatEmptyAsNull": true,
						"skipLineCount": 0,
						"firstRowAsHeader": true
					},
					"fileName": "Products.txt",
					"folderPath": "extracts"
				}
			}
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureSQLDB_TableMarketingStatus')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "LinkedService_SQLDB",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "SQL DB Datasets"
				},
				"type": "AzureSqlTable",
				"structure": [
					{
						"name": "MarketingStatusID",
						"type": "Int32"
					},
					{
						"name": "ApplNo",
						"type": "Int32"
					},
					{
						"name": "ProductNo",
						"type": "Int32"
					}
				],
				"typeProperties": {
					"tableName": "[concat('[', 'dbo].[MarketingStatus]')]"
				}
			}
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureSQLDB_Table_Products')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "LinkedService_SQLDB",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "SQL DB Datasets"
				},
				"type": "AzureSqlTable",
				"structure": [
					{
						"name": "ApplNo",
						"type": "Int32"
					},
					{
						"name": "ProductNo",
						"type": "Int32"
					},
					{
						"name": "Form",
						"type": "String"
					},
					{
						"name": "Strength",
						"type": "String"
					},
					{
						"name": "ReferenceDrug",
						"type": "Int32"
					},
					{
						"name": "DrugName",
						"type": "String"
					},
					{
						"name": "ActiveIngredient",
						"type": "String"
					},
					{
						"name": "ReferenceStandard",
						"type": "Int32"
					}
				],
				"typeProperties": {
					"tableName": "[concat('[', 'dbo].[Products]')]"
				}
			}
		}
	]
}